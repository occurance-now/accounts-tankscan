WindowManager:
	MainWindow:
	SecondWindow:
	ThirdWindow:


<MainWindow>
	name: "main"

	location: location
	date: date 
	time: time 
	air: air 
	tank_type: tank_type
	total_volume: total_volume
	current_volume: current_volume


	GridLayout:
		cols: 1
		size: root.width - 25, root.height - 25
		canvas.before:
		    Color:
		        rgba: ((14/255),(25/255),(80/255), .45)
		    Rectangle:
		        pos: self.pos
		        size: self.size
		Label:
			text: "Newport Biodiesel Portal - Remote Tank Scan"
			color: 1, 1, 1, .8
			font_size: 25
			height: '75dp'
			bold: True
			size_hint_y: None

		GridLayout:
			cols: 3
			height: '50dp'
			padding: 5, 5
			size_hint_y: None

		    Button: 
				text: 'Remote Tank Scan'
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				on_release: 
					app.root.current =  "main"
				
			
			Button: 
				text: 'Accounts'
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				on_release: 
					app.root.current =  "second"
				
			
			Button: 
				text: 'Restaurant Search'
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				on_release: 
					app.root.current =  "third"

		FloatLayout:
			
		    Button: 
		        text: 'Cumberland'
		        halign: 'center'
				valign: 'middle'
				text_size: self.size
				size_hint: (.2, .1)
				pos: 50, 350
		        on_release: root.get_cumberland()

		    Button: 
		        text: 'Shrewsbury'
		        halign: 'center'
				valign: 'middle'
				text_size: self.size
				size_hint: (.2, .1)
				pos: 50, 300
		        on_release: root.get_shrewsbury()
		    Button: 
		        text: 'Merimack'
		        halign: 'center'
				valign: 'middle'
				text_size: self.size
				size_hint: (.2, .1)
				pos: 50, 250
		        on_release: root.get_merimack()

		    Button: 
		        text: 'Portsmouth'
		        halign: 'center'
				valign: 'middle'
				text_size: self.size
				size_hint: (.2, .1)
				pos: 50, 200
		        on_release: root.get_portsmouth()

		    Button: 
		        text: 'Calculate Volume'
		        halign: 'center'
				valign: 'middle'
				text_size: self.size
				size_hint: (.2, .1)
				pos: 50, 75
		        on_release: root.calc_vol()

			Label:
				text: "Remote Depot"
				color: 1, 1, 1, .8
				font_size: 20
				bold: True
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				size_hint: (.2, .1)
				pos: 50, 400

			GridLayout:
				cols: 2
				size_hint: (.4, .5)
				#size: 400, 0
				pos: 325, 200
				padding: 10, 10
				canvas.before:
				    Color:
				        rgba: (1,1,1,.3)
				    Rectangle:
				        pos: self.pos
				        size: self.size
				Label:
					text: "Asset Name: "
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'left'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
		

				Label:
					id: location
					text: ""
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'center'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
			

				Label:
					text: "Date:"
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'left'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
	

				Label:
					id: date
					text: ""
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'center'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
			
				Label:
					text: "Time:"
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'left'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
					

				Label:
					id: time
					text: ""
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'center'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
			

				Label:
					text: "Air Gap:"
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'left'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
				

				Label:
					id: air
					text: ""
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'center'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
	

				Label:
					text: "Tank Type:"
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'left'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)


				Label:
					id: tank_type
					text: ""
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'center'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)


			GridLayout:
				cols: 2
				size_hint: (.4, .2)
				#size: 400, 0
				pos: 325, 50
				padding: 10, 10
				canvas.before:
				    Color:
				        rgba: (1,1,1,.3)
				    Rectangle:
				        pos: self.pos
				        size: self.size
				Label:
					text: "Total Volume:"
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'left'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)

				Label:
					id: total_volume
					text: ""
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'center'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
				

				Label:
					text: "Current Volume:"
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'left'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
				

				Label:
					id: current_volume
					text: ""
					color: 1, 1, 1, .8
					font_size: 20
					halign: 'center'
					valign: 'middle'
					text_size: self.size
					size_hint: (.2, .1)
			

		





<SecondWindow>:
	name: "second"
	restaurant_search: restaurant_search
	restaurant_status: restaurant_status

	GridLayout:
		cols: 1
		size: root.width - 25, root.height - 25
		canvas.before:
		    Color:
		        rgba: ((14/255),(25/255),(80/255), .45)
		    Rectangle:
		        pos: self.pos
		        size: self.size
		Label:
			text: "Newport Biodiesel Portal - Accounts"
			color: 1, 1, 1, .8
			font_size: 25
			height: '75dp'
			bold: True
			size_hint_y: None

		GridLayout:
			cols: 3
			height: '50dp'
			padding: 5, 5
			size_hint_y: None

		    Button: 
				text: 'Remote Tank Scan'
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				on_release: 
					app.root.current =  "main"
					root.manager.transition.direction = 'left'
				
			
			Button: 
				text: 'Accounts'
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				on_release: 
					app.root.current =  "second"
					root.manager.transition.direction = 'left'
				
			
			Button: 
				text: 'Restaurant Search'
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				on_release: 
					app.root.current =  "third"
					root.manager.transition.direction = 'left'
		FloatLayout:
			Label:
				text: "Search Restaurant Name"
				color: 1, 1, 1, 1
				font_size: 18
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				size_hint: (.4, .1)
				pos: 95, 355

			TextInput:
				id: restaurant_search
				multinline:False
				size_hint: (.4, .09)
				pos: 125, 310

			Button:
				id: restaurant_search_button
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				text: 'Search Restaurant Data Base'
				size_hint: (.3, .1)
				pos: 460, 308
				on_press: root.current_account()

			Label:
				id: restaurant_status
				font_size: 18
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				text:''
				size_hint: (.4, .1)
				pos: 80, 250

			Button:
				halign: 'center'
				valign: 'middle'
				text_size: self.size
				text: 'Clear Status'
				size_hint: (.3, .1)
				pos: 460, 258
				on_press: root.clear_status()

			GridLayout:
				cols: 2
				size_hint: (.5, .2)
				pos: 200, 10
				padding: 20, 20

				Button:
					text: 'Upload to Master List'
					halign: 'center'
					valign: 'middle'
					text_size: self.size
					on_press: root.open_upload()
				Button: 
					text: 'Google Link'
					halign: 'center'
					valign: 'middle'
					text_size: self.size
					on_press: root.open_google()


